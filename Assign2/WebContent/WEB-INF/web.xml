    <description></description>
    <display-name>HotelController</display-name>
    <servlet-name>HotelController</servlet-name>
    <servlet-class>mvcController.HotelController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HotelController</servlet-name>
    <url-pattern>/HotelController</url-pattern>
  </servlet-mapping>
